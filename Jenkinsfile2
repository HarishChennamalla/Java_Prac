pipeline
{
    agent any
    tools
        {
            maven 'MAVEN'
        }
    stages
    {
        stage('One')
        {
            steps
            {
                sh 'mvn clean package'
            }
        }
        stage('Two')
        {
            steps
            {
            withSonarQubeEnv('sonar')
            {
                sh 'mvn sonar:sonar'
            }

            }
        }
    }
}
