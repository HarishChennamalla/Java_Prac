pipeline
{
    agent any
    stages
    {
        stage('One')
        {
            steps
            {
                sh 'mvn clean install'
            }
        }
        stage('Two')
        {
            steps
            {
                sh 'sonar-scanner -Dsonar.projectKey="javap" -Dsonar.sources=. -Dsonar.host.url=http://172.31.13.96:9000 -Dsonar.login=admin -Dsonar.password=admin123'
            }
        }
    }
}
