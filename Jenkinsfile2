pipeline
{
    agent none
    stages
    {
        stage('One')
        {
            agent
            {
            label 'MAVEN_NODE'
            }
            steps
            {

                sh 'mvn -version'
            }
        }
        stage('Two')
        {
        agent
        {
            label 'MAVEN_NODE'
        }
            steps
            {
                sh 'echo Welcome to stage2'
            }
        }
    }
}
